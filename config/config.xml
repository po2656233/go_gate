            <!--				<node ip="0.0.0.0" port="8754" maxload="3000" enable="true">多子游戏服</node>-->
            <!--				<node ip="0.0.0.0" port="87599" maxload="3000" enable="true">多子游戏服</node>-->
            <!--				<node ip="0.0.0.0" port="8758" maxload="3000" enable="true">多子游戏服</node>-->
            <!--			</line>-->
        </busline>
        <busline name="tcp" addr=":20002" type="tcp" redirect="" tls="false" realipmode="tcp">
            <line serverid="Login">
                <node ip="127.0.0.1" port="10021" maxload="50000" enable="false">tcp登录服</node>
            </line>
        </busline>
    </proxy>
    <api addr=":10001" type="http" username="##sss^^^" password="(S?SS&amp;^.14" registerpath="/register" querypath="/info" reloadpath="/reload"
