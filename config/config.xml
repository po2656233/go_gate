<setting>
    <!--基础配置-->
    <options debug="true" logdir="./logs/" redirect="false" ctrlkey="^^*SDASD)A)$%">
        <heartbeat interval="20" timeout="50">时间间隔</heartbeat>
    </options>
    <!--开启nacos的服务发现后,优先走nacos节点-->
    <nacos enable="true" level="debug" logdir="/tmp/nacos/log" cachedir="/tmp/nacos/cache" contextpath="/nacos">
        <item ip="121.37.211.227" port="8848" namespaceid="eaa4c5d3-d6a5-4502-9deb-a4875decd950" groupname="GAME_GROUP" clusters="Games" maxload="1"/>
    </nacos>
    <!--当nacos不启用时，走代理配置-->
    <proxy>
        <busline name="ws" addr=":9950" type="websocket" redirect="" tls="false" realipmode="http">
            <route path="/">子路由</route>
            <line serverid="login">
                <node ip="127.0.0.1" port="9958" maxload="1" enable="false">登录服</node>
            </line>
            <line serverid="login">
                <node ip="127.0.0.1" port="9970" maxload="2" enable="true">子游戏服</node>
            </line>
            <!--			<line serverid="chat">-->
            <!--				<node ip="0.0.0.0" port="8752" maxload="3000" enable="true">聊天服</node>-->
            <!--			</line>-->
            <!--			<line serverid="zjh3">-->
